{
  "name": "headless-usepagination",
  "version": "0.1.2",
  "private": false,
  "author": "Alex Didenko",
  "license": "MIT",
  "description": "Data pagination hooks for react",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex461919/headless-usepagination.git"
  },
  "bugs": {
    "url": "https://github.com/alex461919/headless-usepagination/issues"
  },
  "peerDependencies": {
    "react": ">=16.9.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "usePagination",
    "pagination"
  ],
  "devDependencies": {
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
